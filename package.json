{
  "name": "setIt-backend",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:brunofow/setIt-backend.git",
  "author": "Bruno <brunocamposfow@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "babel src --extensions \".js,.ts\" --out-dir dist --copy-files --no-copy-ignored",
    "dev": "ts-node-dev -r tsconfig-paths/register --transpile-only --ignore-watch node_module --respawn --no-notify src/server.ts",
    "migrate:latest": "knex --knexfile knexfile.ts migrate:latest",
    "migrate:rollback": "knex --knexfile knexfile.ts migrate:rollback",
    "test": "cross-env NODE_ENV=test jest"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.1",
    "@babel/node": "^7.10.5",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-typescript": "^7.10.4",
    "@types/bcrypt": "^3.0.0",
    "@types/cors": "^2.8.7",
    "@types/express": "^4.17.7",
    "@types/multer": "^1.4.3",
    "@types/nodemailer": "^6.4.0",
    "@types/passport-jwt": "^3.0.3",
    "@types/supertest": "^2.0.10",
    "babel-plugin-module-resolver": "^4.0.0",
    "jest": "^26.4.0",
    "supertest": "^4.0.2",
    "ts-jest": "^26.2.0",
    "ts-node-dev": "^1.0.0-pre.56",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.9.7"
  },
  "dependencies": {
    "bcrypt": "^5.0.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jwt-simple": "^0.5.6",
    "knex": "^0.21.4",
    "multer": "^1.4.2",
    "nodemailer": "^6.4.11",
    "nodemailer-express-handlebars": "^4.0.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "sqlite3": "^5.0.0"
  }
}
